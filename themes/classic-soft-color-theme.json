{
	"name": "classic-soft",
	"colors": {
		"editor.background": "#212124",
		"editor.foreground": "#C0C0C0",
		"editor.lineHighlightBackground": "#1b1b1b",
		"editor.selectionBackground": "#253550",
		
    "editorCursor.foreground": "#D0D0D0",
		"editorLineNumber.activeForeground": "#C0C0C0",
		"editorLineNumber.foreground": "#707070",
		"editorWhitespace.foreground": "#4e4e4e",
		"editorUnnecessaryCode.opacity": "#00000050",
    
    "editorIndentGuide.background": "#444444",
    "editorIndentGuide.activeBackground": "#9D550FB0"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#696969"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#50ad50ff"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ad5dc8ff"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#6b6bc8ff",
				"fontStyle": " bold italic"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#936bc8ff"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": " italic",
				"foreground": "#c85093ff"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#bbbb50ff"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#d0205fff"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#50bbbbff"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#97D618"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#75A516"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#50c885ff"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c87843ff"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#d0205fff"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#8abc26ff"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#50c885ff"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#85c850ff"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c86b6bff"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": " italic",
				"foreground": "#c85093ff"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#cfcfc8ff"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#cfcfc8ff"
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#adada2ff"
			}
		},
		{
			"name": "diff.header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#625e4eff"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#d0205fff"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#8abc26ff"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#c0b761ff"
			}
		},
		{
			"name": "GitGutter deleted",
			"scope": "markup.deleted.git_gutter",
			"settings": {
				"foreground": "#d0205fff"
			}
		},
		{
			"name": "GitGutter inserted",
			"scope": "markup.inserted.git_gutter",
			"settings": {
				"foreground": "#8abc26ff"
			}
		},
		{
			"name": "GitGutter changed",
			"scope": "markup.changed.git_gutter",
			"settings": {
				"foreground": "#c0b761ff"
			}
		},
		{
			"name": "GitGutter ignored",
			"scope": "markup.ignored.git_gutter",
			"settings": {
				"foreground": "#625e4eff"
			}
		},
		{
			"name": "GitGutter untracked",
			"scope": "markup.untracked.git_gutter",
			"settings": {
				"foreground": "#625e4eff"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#916cd5a1"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#c0b761ff"
			}
		}
	]
